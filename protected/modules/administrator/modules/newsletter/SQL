  ADD CONSTRAINT `rmp_log_ibfk_2` FOREIGN KEY (`newsletter_id`) REFERENCES `rmp_newsletter` (`id`) ON DELETE CASCADE ON UPDATE CASCADE,
  ADD CONSTRAINT `rmp_log_ibfk_1` FOREIGN KEY (`subscriber_id`) REFERENCES `rmp_subscribers` (`id`) ON DELETE CASCADE ON UPDATE CASCADE;

--
-- Constraints for table `rmp_queue`
--
ALTER TABLE `rmp_queue`
  ADD CONSTRAINT `rmp_queue_ibfk_2` FOREIGN KEY (`newsletter_id`) REFERENCES `rmp_newsletter` (`id`) ON DELETE CASCADE ON UPDATE CASCADE,
  ADD CONSTRAINT `rmp_queue_ibfk_3` FOREIGN KEY (`subscriber_id`) REFERENCES `rmp_subscribers` (`id`) ON DELETE CASCADE ON UPDATE CASCADE;

--
-- Constraints for table `rmp_subscribers_list`
--
ALTER TABLE `rmp_subscribers_list`
  ADD CONSTRAINT `rmp_subscribers_list_ibfk_1` FOREIGN KEY (`subscriber_id`) REFERENCES `rmp_subscribers` (`id`) ON DELETE CASCADE ON UPDATE CASCADE,
  ADD CONSTRAINT `rmp_subscribers_list_ibfk_2` FOREIGN KEY (`list_id`) REFERENCES `rmp_list` (`id`) ON DELETE CASCADE ON UPDATE CASCADE;






